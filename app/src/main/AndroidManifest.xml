<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.narendra.advance">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Camera"
            android:label="@string/title_activity_camera"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Camara_test"
            android:label="@string/title_activity_camara_test"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CamaraNew"
            android:label="@string/title_activity_camara_new"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CameraPreview"
            android:label="@string/title_activity_camera_preview"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>